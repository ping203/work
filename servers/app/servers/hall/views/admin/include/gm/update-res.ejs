<div class="col-md-6">
  <section class="panel panel-collapsed">
    <header class="panel-heading">
      <div class="panel-actions">
        <a href="#" class="panel-action panel-action-toggle" data-panel-toggle=""></a>
        <a href="#" class="panel-action panel-action-dismiss" data-panel-dismiss=""></a>
      </div>

      <h2 class="panel-title">
        <%= txt_title_update_res %>
        </h2>
      <p class="panel-subtitle">
        <%= txt_content_update_res %>
        </p>
    </header>
    <div class="panel-body">

      <!-- content start -->
      <form name="upload-res" id="upload-res" method="post" enctype="multipart/form-data" action="/admin_api/upload_res">
        <div class="form-group mb-lg">
          <label>请选择要上传的文件</label>
        </div>
        <div class="form-group mb-lg">
          <input type="file" name="file" />
        </div>
        <div class="form-group mb-lg">
          <label>请选择资源分类</label>
        </div>
        <div class="form-group mb-lg">
          <label>
            <input name="raw_type" type="radio" value="assets" checked="checked" />assets
          </label>
        </div>
        <div class="form-group mb-lg">
          <label>
            <input name="raw_type" type="radio" value="internal" />internal
          </label>
        </div>
        <div class="form-group mb-lg">
          <label>请选择服务器路径</label>
        </div>
        <div id="container_path_selector" class="form-group mb-lg">
          <select id="path_assets_list" name="path" form="upload-res" class="form-control mb-md">
            <% for(var i = 0; i
            < path_assets_list.length; i++){ %>
            <option>
              <%= path_assets_list[i] %>
            </option>
            <% } %>
          </select>
          <select id="path_internal_list" name="path" form="upload-res" class="form-control mb-md">
            <% for(var i = 0; i
            < path_internal_list.length; i++){ %>
            <option>
              <%= path_internal_list[i] %>
            </option>
            <% } %>
          </select>
        </div>
        <div class="form-group mb-lg">
          <input type="submit" value="提交" />
        </div>
      </form>
      <!-- content end -->
      
    </div>
  </section>
</div>