<div class="col-md-6">
  <section class="panel panel-collapsed">
    <header class="panel-heading">
      <div class="panel-actions">
        <a href="#" class="panel-action panel-action-toggle" data-panel-toggle=""></a>
        <a href="#" class="panel-action panel-action-dismiss" data-panel-dismiss=""></a>
      </div>

      <h2 class="panel-title">
        <%= txt_title_bind %>
      </h2>
      <p class="panel-subtitle">
        <%= txt_content_bind %>
        </p>
    </header>
    <div class="panel-body">

      <form id="get-temp-account">
        <div class="form-group mb-lg">
          <label>临时账号</label>
          <input id="input-temp-account-tobe-bind" name="tempname" type="text" class="form-control input-lg" />
        </div>

        <div class="form-group mb-lg">
          <label>输入昵称</label>
          <input id="input-bind-nickname" name="nickname" type="text" class="form-control input-lg" />
        </div>

        <div class="form-group mb-lg">
          <label>输入密码</label>
          <input id="input-bind-password" name="password" type="password" class="form-control input-lg" />
        </div>
      </form>
      <button id="btn-bind" type="submit" class="btn btn-primary hidden-xs">绑定昵称</button>
      
    </div>
  </section>
</div>