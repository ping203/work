<div class="col-md-6">
  <section class="panel panel-collapsed">
    <header class="panel-heading">
      <div class="panel-actions">
        <a href="#" class="panel-action panel-action-toggle" data-panel-toggle=""></a>
        <a href="#" class="panel-action panel-action-dismiss" data-panel-dismiss=""></a>
      </div>

      <h2 class="panel-title">
        <%= txt_title_account %>
      </h2>
      <p class="panel-subtitle">
        <%= txt_content_account %>
        </p>
    </header>
    <div class="panel-body">
      
		<!-- Form: Logout Account -->
		<form>
			<div class="form-group mb-lg">
			<label>输入用户ID</label>
			<input id="input-account-id" name="account_id" type="text" class="form-control input-lg account-id" />
			</div>
			<div class="form-group mb-lg">
			<label>输入用户Token</label>
			<input id="input-account-token" name="token" type="text" class="form-control input-lg account-token" />
			</div>
		</form>

		<button id="btn-logout-account" type="submit" class="btn btn-primary hidden-xs">退出登录</button>
		<button id="btn-get-day-reward" class="btn btn-primary hidden-xs">领取签到奖励</button>
		<button id="btn-get-online-time" class="btn btn-primary hidden-xs">获取当日在线时间</button>
    <button id="btn-get-broke" class="btn btn-primary hidden-xs">破产领取</button>
      
    </div>
  </section>
</div>