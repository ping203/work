<!doctype html>
<html class="fixed">
	<head>
    <% include include/head.ejs %>
	</head>
	<body>
		<section class="body">

			<!-- start: header -->
      <% include include/header.ejs %>
			<!-- end: header -->

			<div class="inner-wrapper">
				<!-- start: sidebar -->
        <% include include/sidebar.ejs %>
				<!-- end: sidebar -->

				<section role="main" class="content-body">
					<header class="page-header">
						<h2>
              <%= txt_register %>
            </h2>
					</header>

					<!-- start: page -->

          <div class="row">
            <% include include/date-selector.ejs %>
          </div>

          <div class="row">
            <div class="col-md-12">
              <section class="panel">
                <header class="panel-heading">
                  <div class="panel-actions">
                    <a href="#" class="panel-action panel-action-toggle" data-panel-toggle=""></a>
                    <a href="#" class="panel-action panel-action-dismiss" data-panel-dismiss=""></a>
                  </div>

                  <h2 class="panel-title">
                    <%= txt_title_new_account %>
                  </h2>
                  <p class="panel-subtitle">
                    <%= txt_content_new_account %>
                  </p>
                </header>
                <div class="panel-body" id="panel-new-account">
                  <!-- TODO -->
                  <div class="chart chart-md" id="flot-bars-new-account"></div>
                </div>
              </section>
            </div>
          </div>

          <div class="row">
            <div class="col-md-12">
              <section class="panel panel-collapsed">
                <header class="panel-heading">
                  <div class="panel-actions">
                    <a href="#" class="panel-action panel-action-toggle" data-panel-toggle=""></a>
                    <a href="#" class="panel-action panel-action-dismiss" data-panel-dismiss=""></a>
                  </div>

                  <h2 class="panel-title">
                    <%= txt_title_new_bind %>
                  </h2>
                  <p class="panel-subtitle">
                    <%= txt_content_new_bind %>
                  </p>
                </header>
                <div class="panel-body" id="panel-bind-account">
                  <!-- TODO -->
                  <div class="chart chart-md" id="flot-bars-bind-count"></div>
                </div>
              </section>
            </div>
          </div>

					<!-- end: page -->
				</section>
			</div>

		</section>
    <% include include/script.ejs %>

    <!-- Page Related -->
      <% include include/admin_script.ejs %>
    <script src="../fjds/javascript/util/date_selector.js"></script>
    <script src="../fjds/javascript/admin/pages_register.js"></script>
	</body>
</html>